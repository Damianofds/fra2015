<?xml version="1.0" encoding="UTF-8"?>
<rules>
    <rule>
        <id>1</id>
        <description>
            textual description for this rule
        </description>
        <entryId>1a</entryId>
        <error>
           <![CDATA[ <label ref="rule_error_1" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{1.7}} > {{1.7.1}} ]]>
        </condition>
    </rule>
    <rule>
        <id>2</id>
        <description>
            1.1+1.2+1.3=land area (compare/check with external data)
        </description>
        <entryId>1a</entryId>
        <error>
           <![CDATA[ <label ref="rule_error_2" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{1.1}} + {{1.2}}+ {{1.3}} === LAND_AREA ]]>
        </condition>
    </rule> 
    <rule>
        <id>3</id>
        <description>
            1.1 change rate1 in % should be between +3.00% and -3.00% (?)
        </description>
        <entryId>1a</entryId>
        <error>
           <![CDATA[ <label ref="rule_error_3" /> ]]>
        </error>
        <condition>
            <![CDATA[ undefined ]]>
        </condition>
    </rule>          
    <rule>
        <id>4</id>
        <description>
            1.4=1.5-(1.1+1.2+1.3)
        </description>
        <entryId>1a</entryId>
        <error>
           <![CDATA[ <label ref="rule_error_4" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{1.4}} === {{1.5}} - ({{1.1}}+{{1.2}}+{{1.3}}) ]]>
        </condition>
    </rule>  
    <rule>
        <id>5</id>
        <description>
            1.5= Country area (compare/check with external data)
        </description>
        <entryId>1a</entryId>
        <error>
           <![CDATA[ <label ref="rule_error_5" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{1.5}} === COUNTRY_AREA + 0 ]]>
        </condition>
    </rule>    
    <rule>
        <id>7</id>
        <description>
            1.7 not greater that net change in Forest area (1.1)/year (?)
        </description>
        <entryId>1a</entryId>
        <error>
           <![CDATA[ <label ref="rule_error_7" /> ]]>
        </error>
        <condition>
            <![CDATA[ undefined ]]>
        </condition>
    </rule> 
    <rule>
        <id>8</id>
        <description>
            1.6 + 1.8 not greater than net change in Forest area (1.1) (?)
        </description>
        <entryId>1a</entryId>
        <error>
           <![CDATA[ <label ref="rule_error_8" /> ]]>
        </error>
        <condition>
            <![CDATA[ undefined ]]>
        </condition>
    </rule> 
    <rule>
        <id>9</id>
        <description>
            2.1 change rate in % should be between +3.00% and -3.00% (?)
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ undefined ]]>
        </condition>
    </rule>  
    <rule>
        <id>10</id>
        <description>
            2.1+2.2+2.3 = 1.1
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{2.1}} + {{2.2}} + {{2.3}} === {{1.1}} ]]>
        </condition>
    </rule>          
    <rule>
        <id>11</id>
        <description>
            3.1/1.1= usually within a range between 20 and 300
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{3.1}}/{{1.1}}<=300 && {{3.1}}/{{1.1}}>=20 ]]>
        </condition>
    </rule> 
    <rule>
        <id>12</id>
        <description>
            3.1.1+3.1.2=3.1
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{3.1.1}} + {{3.1.2}} === {{3.1}} ]]>
        </condition>
    </rule>    
    <rule>
        <id>13</id>
        <description>
            3.2=usually within a range between 5 and 100
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{3.2}} >= 5 && {{3.2}}<=100 ]]>
        </condition>
    </rule>  
    <rule>
        <id>14</id>
        <description>
            3.2.1+3.2.2=3.2
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{3.2.1}} + {{3.2.2}} === {{3.2}} ]]>
        </condition>
    </rule>  
    <rule>
        <id>15</id>
        <description>
            3.3 (total)=3.1
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{3.3}} === {{3.1}} + 0 ]]>
        </condition>
    </rule>    
    <rule>
        <id>16</id>
        <description>
            3.6/3.5=range between 0.1 and 0.5
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{3.6}}/{{3.5}} >= 0.1 && {{3.6}}/{{3.5}} <= 0.5 ]]>
        </condition>
    </rule>  
    <rule>
        <id>17</id>
        <description>
            3.7/(3.5+3.6)=range between 0 and 0.2
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{3.7}}/({{3.5}}+{{3.6}}) >= 0 && {{3.7}}/({{3.5}}+{{3.6}}) <= 0.2 ]]>
        </condition>
    </rule> 
    <rule>
        <id>18</id>
        <description>
            3.8=3.5*0.47 (usually around 0.5 of the Biomass estimates and should be similar over the years)
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{3.8}} === {{3.5}}*0.47 ]]>
        </condition>
    </rule>   
    <rule>
        <id>19</id>
        <description>
            (3.8+3.9)/3.1= range between 0.4 and 10
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ ( {{3.8}} + {{3.9}} ) / {{3.1}}  >= 0.4 && ( {{3.8}} + {{3.9}} ) / {{3.1}} <= 10 ]]>
        </condition>
    </rule>   
    <rule>
        <id>20</id>
        <description>
            3.9=3.6*0.47 (usually around 0.5 of the Biomass estimates and should be similar over the years)
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{3.9}} === {{3.6}}*0.47 ]]>
        </condition>
    </rule>  
    <rule>
        <id>21</id>
        <description>
            3.10=3.7*0.47 (usually around 0.5 of the Biomass estimates and should be similar over the years)
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{3.10}} === {{3.7}}*0.47 ]]>
        </condition>
    </rule>    
    <rule>
        <id>22</id>
        <description>
            3.12/1.1 = usually within a range of 30 to 100
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{3.12}}/{{1.1}} >= 30 && {{3.12}}/{{1.1}} <= 100 ]]>
        </condition>
    </rule>   
    <rule>
        <id>23</id>
        <description>
            The sum of (4.1+4.2+4.3) should be less than or equal to 1.1
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{3.1}} + {{4.2}} + {{4.3}} <= {{1.1}} ]]>
        </condition>
    </rule>       
    <rule>
        <id>24</id>
        <description>
            5.1 should be less than or equal to 1.1
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{5.1}} <= {{1.1}} ]]>
        </condition>
    </rule>  
    <rule>
        <id>25</id>
        <description>
            5.2 should be less than or equal to 1.1
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{5.2}} <= {{1.1}} ]]>
        </condition>
    </rule> 
    <rule>
        <id>26</id>
        <description>
            6.1 should be less than 1.1
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{6.1}} < {{1.1}} ]]>
        </condition>
    </rule>  
    <rule>
        <id>26</id>
        <description>
            6.2 should be less than 1.1
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{6.2}} < {{1.1}} ]]>
        </condition>
    </rule>        
    <rule>
        <id>27</id>
        <description>
            8.1 should be less than 1.1+1.2+1.3
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{8.1}} < {{1.1}}+{{1.2}}+{{1.3}} ]]>
        </condition>
    </rule>          
    <rule>
        <id>28</id>
        <description>
            12.1 should be less than or equal to 1.1
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{12.1}} <= {{1.1}} ]]>
        </condition>
    </rule>   
    <rule>
        <id>29</id>
        <description>
            14.1 should be less than or equal to 1.1
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{14.1}} <= {{1.1}} ]]>
        </condition>
    </rule>   
    <rule>
        <id>30</id>
        <description>
            14.1.1 + 14.1.2=14.1
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{14.1.1}} + {{14.1.2}} === {{14.1}} ]]>
        </condition>
    </rule>        
    <rule>
        <id>31</id>
        <description>
            14.3 should be less than or equal to 100
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{14.3}} <= 100 ]]>
        </condition>
    </rule>       
    <rule>
        <id>32</id>
        <description>
            18.1+18.2+18.3=1.1
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{18.1}} + {{18.2}} + {{18.3}} === {{1.1}} ]]>
        </condition>
    </rule>  
    <rule>
        <id>33</id>
        <description>
            18.1=18.4+18.5+18.6+18.7+18.8
        </description>
        <entryId>1a</entryId>
        <error>
            <![CDATA[ <label ref="ref_to_error_message" /> ]]>
        </error>
        <condition>
            <![CDATA[ {{18.1}} === {{18.4}} + {{18.5}} + {{18.6}} + {{18.7}} + {{18.8}} ]]>
        </condition>
    </rule>                                                      
</rules>
